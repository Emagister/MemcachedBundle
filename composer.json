{
    "name": "emagister/memcached-bundle",
    "description": "A Symfony2 bundle to handle memcached connections",
    "type": "library",
    "license": "MIT",
    "require": {
        "ext-memcache": "*",
        "ext-memcached": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.x-dev",
        "mockery/mockery": "dev-master",
        "symfony/symfony": "2.1.x-dev"
    },
    "suggest": {
        "ext-igbinary": "Use high performant serialization to save data to memcache"
    },
    "authors": [
        {
            "name": "Christian Soronellas",
            "email": "csoronellas@emagister.com"
        }
    ],
    "minimum-stability": "dev",
    "target-dir": "Emagister/Bundle/MemcachedBundle",
    "autoload": {
        "psr-0": {
            "Emagister\\Bundle\\MemcachedBundle": ""
        }
    }
}
