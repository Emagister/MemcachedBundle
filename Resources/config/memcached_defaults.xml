<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>

        <parameter key="emagister_memcached.memcache.class">Doctrine\Common\Cache\MemcacheCache</parameter>
        <parameter key="emagister_memcached.memcache_host">localhost</parameter>
        <parameter key="emagister_memcached.memcache_port">11211</parameter>
        <parameter key="emagister_memcached.memcached.class">Doctrine\Common\Cache\MemcachedCache</parameter>
        <parameter key="emagister_memcached.memcached_host">localhost</parameter>
        <parameter key="emagister_memcached.memcached_port">11211</parameter>

    </parameters>

    <services>

        <service id="emagister_memcached.memcache" class="%emagister_memcached.memcache.class%" abstract="true" />
        <service id="emagister_memcached.memcached" class="%emagister_memcached.memcached.class%" abstract="true" />

    </services>

</container>